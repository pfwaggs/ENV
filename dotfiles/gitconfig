[filter "lfs"]
	clean = git lfs clean %f
	smudge = git lfs smudge %f
	required = true
[user]
	name = Pnomohr Waggs
	email = pfwaggs@gmail.com
[core]
	autocrlf = input
[alias]
	st = status -s
	lg = log --oneline -n 10 --graph --decorate
	co = checkout
	ci = commit
	bl = branch --list
	hist = log -n 10 --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
[push]
	default = simple
[credential]
	helper = cache --timeout=3600
