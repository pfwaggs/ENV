# vim: fdc=4 fmr=AAA,ZZZ fdm=marker

ew () {
    declare -A macs=(
        [dirtnap]=08:62:66:c7:a8:cb
    )
    [[ ${macs[$1]} ]] || echo $1 is not a valid machine
    if [[ ${macs[$1]} ]]; then
        sudo etherwake ${macs[$1]}
    else
        echo $1 is not a valid machine
    fi
}
