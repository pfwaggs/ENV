
function _ParseArg () {
    declare x cdlist
    [[ -v COMP_LINE ]] && x=$2 || x=$1
    #[[ $x =~ , ]] && x=$(_FindPath $x)
    declare -a list
    for dir in ${CDPATH//:/ } $(dirs -l); do
	dir=$(eval realpath $dir)
	for file in $(compgen -f "$dir/$x"); do
	    [[ ${list[@]} =~ $file ]] && continue
	    list+=("$file")
	done
    done
    #[[ ${#list[@]} -gt 0 ]] || list+=("$x")
    for x in "${list[@]}"; do echo $x; done
}

