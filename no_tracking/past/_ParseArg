
function _ParseArg () { #AzA
    declare x cdlist
    [[ -v COMP_LINE ]] && x=$2 || x=$1
    declare -a list
    for dir in ${CDPATH//:/ } $(dirs -l); do
	dir=$(eval realpath $dir)
	for file in $(compgen -d "$dir/$x"); do
	    list+=("$file")
	done
    done
    for x in "${list[@]}"; do echo $x; done
} #ZaZ

