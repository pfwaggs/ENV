function typef () { #AzA
    declare -a p;
    type -a $1 | while read -a p; do
	file ${p[-1]};
    done;
} #ZaZ

function typel () { #AzA
    declare -a p;
    type -a $1 | while read -a p; do
	ls -l ${p[-1]};
    done;
} #ZaZ

function typel () { #AzA
    declare -a p;
    type -a $1 | while read -a p; do
	less ${p[-1]};
    done;
} #ZaZ
